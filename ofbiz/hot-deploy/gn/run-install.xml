<?xml version="1.0" encoding="UTF-8"?>

<project name="Data Install" default="run-install-seed" basedir="../../">

    <target name="run-install-beta-test" description="load beta test data">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="beta-test-data"/>
        </subant>
    </target>

    <target name="run-install-load-test" description="load load test data">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="load-test-data"/>
        </subant>
    </target>

    <target name="run-install-poc" description="load poc data">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="poc-data"/>
        </subant>
    </target>

    <target name="run-install-seed" description="load seed data">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="seed"/>
        </subant>
    </target>

    <target name="run-install-seed-init-data" description="load seed data">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="seed-init-data"/>
        </subant>
    </target>

    <target name="run-install-test" description="load test data">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="test-data"/>
        </subant>
    </target>

    <target name="run-install-propeties" description="load propeties">
        <subant target="run-install-readers" buildpath="." antfile="build.xml" >
            <property name="data-readers" value="propeties"/>
        </subant>
    </target>
</project>