#* @vtlvariable name="export" type="java.lang.Boolean" *#
#* @vtlvariable name="paramDescriptions" type="java.util.Map"  *#
#* @vtlvariable name="modelService" type="org.ofbiz.service.ModelService" *#
#* @vtlvariable name="serviceName" type="java.lang.String" *#
<html>
<head></head>
<body>
<div style="font-size: x-large"><b>$serviceName</b></div>
<div style="font-size: large">$modelService.description</div>

    <div style="font-size: large">service is #if (!$export) not#end exported</div>
<br>

<div style="font-size: large"><b>IN parameters</b></div>
<table title="IN" border="1" width="80%">
    <tr>
        <td><b>name</b></td>
        <td><b>optional</b></td>
        <td><b>type</b></td>
        <td><b>mode</b></td>
        <td><b>entity</b></td>
        <td><b>description</b></td>
    </tr>
    #foreach($pName in $modelService.getInParamNames())
        #set( $p=$modelService.getParam($pName))
        <tr>
            <td>$pName</td>
            <td>#if($p.isOptional()) true #else false #end</td>
            <td>$p.getType()</td>
            <td>$p.getMode()</td>
            <td>#if ($p.getEntityName()) $p.getEntityName()#end</td>
        ##            <td>#if ($p.description) $p.description()#end</td>
            <td>$paramDescriptions.get($pName)</td>
        </tr>
    #end


</table>
<br>

<div style="font-size: large"><b>OUT parameters</b></div>
<table title="IN" border="1" width="80%">
    <tr>
        <td><b>name</b></td>
        <td><b>optional</b></td>
        <td><b>type</b></td>
        <td><b>mode</b></td>
        <td><b>entity</b></td>
        <td><b>description</b></td>
    </tr>
    #foreach($pName in $modelService.getOutParamNames())
        #set( $p=$modelService.getParam($pName))
        <tr>
            <td>$pName</td>
            <td>#if($p.isOptional()) true #else false #end</td>
            <td>$p.getType()</td>
            <td>$p.getMode()</td>
            <td>#if ($p.getEntityName()) $p.getEntityName()#end</td>
        ##            <td>#if ($p.description) $p.description()#end</td>
            <td>$paramDescriptions.get($pName)</td>
        </tr>
    #end
</table>