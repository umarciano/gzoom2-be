<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Gn Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="paginatedFindListInterface" engine="interface">
        <attribute name="viewIndex" type="Integer" mode="IN" optional="true"/>
        <attribute name="viewSize" type="Integer" mode="IN" optional="true"/>
        <attribute name="list" type="List" mode="OUT" optional="true"/>
        <attribute name="listSize" type="Integer" mode="OUT" optional="true"/>
    </service>

    <service name="gnPing" engine="java" export="true"
             location="org.ofbiz.common.CommonServices" invoke="ping" auth="true">
        <description>Test Ping Service (authentication needed)</description>
        <attribute name="message" type="String" mode="INOUT" optional="true"/>
    </service>

    <service name="gnGenerateDocumentation" engine="java" export="false"
             location="it.memelabs.gn.doc.GnGenerateDocumentation" invoke="gnGenerateDocumentation" auth="false">
        <description>Test Ping Service (authentication needed)</description>
    </service>

    <service name="gnAuditEventInterface" engine="interface" invoke="">
        <description>interface that return audit events</description>
        <attribute mode="OUT" name="auditEvents" type="Map" optional="true"/>
    </service>

    <service name="gnReadOnlyInterface" engine="interface" invoke="">
        <description>interface that set readonly service call</description>
        <attribute mode="IN" name="readOnly" type="java.lang.String" optional="true" default-value="N"/>
    </service>


</services>