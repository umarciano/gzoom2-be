<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Gn Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="gnUpdateUserLoginOriginId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnUpdateUserLoginOriginId"
             export="false">
        <description>Update userLogin origin</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="originId" type="String" mode="IN" optional="false"/>
    </service>

    <!-- <service name="gnUpdateUserLoginLoginSourceId" engine="java" auth="false"
              location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnUpdateUserLoginLoginSourceId"
              export="false">
         <description>Update userLogin origin</description>
         <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
         <attribute name="loginSourceId" type="String" mode="IN" optional="false"/>
     </service>-->

    <service name="gnFindLoginSourceId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnFindLoginSourceId"
             export="false">
        <description>Find loginSourceIds</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="loginSourceIds" type="java.util.List" mode="OUT" optional="false"/>
    </service>


    <service name="gnCheckLoginSourceId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnCheckLoginSourceId"
             export="false">
        <description>Check passed loginSourceId</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="loginSourceId" type="String" mode="IN" optional="false"/>
        <attribute name="success" type="String" mode="OUT" optional="false"/>
    </service>

    <service name="gnAddUserLoginLoginSourceId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnAddUserLoginLoginSourceId"
             export="false">
        <description>addUserLoginLoginSourceId</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="loginSourceId" type="String" mode="IN" optional="false"/>
    </service>


    <service name="gnCreateUserLoginDeviceUniqueId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnCreateUserLoginDeviceUniqueId"
             export="false">
        <description>gnCreateUserLoginDeviceUniqueId</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="deviceId" type="String" mode="IN" optional="false"/>
        <attribute name="isActive" type="String" mode="IN" optional="false"/>

        <attribute name="deviceDescription" type="String" mode="IN" optional="true"/>
        <attribute name="appId" type="String" mode="IN" optional="true"/>
        <attribute name="appVersion" type="String" mode="IN" optional="true"/>
    </service>

    <service name="gnAddActiveUserLoginDeviceUniqueId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnAddActiveUserLoginDeviceUniqueId"
             export="false">
        <description>Add new device and set oldest inactive</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="deviceId" type="String" mode="IN" optional="false"/>

        <attribute name="deviceDescription" type="String" mode="IN" optional="true"/>
        <attribute name="appId" type="String" mode="IN" optional="true"/>
        <attribute name="appVersion" type="String" mode="IN" optional="true"/>
    </service>

    <service name="gnUpdateUserLoginDeviceUniqueId" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnUpdateUserLoginDeviceUniqueId"
             export="false">
        <description>gnUpdateUserLoginDeviceUniqueId</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="false"/>
        <attribute name="deviceId" type="String" mode="IN" optional="false"/>

        <attribute name="deviceDescription" type="String" mode="IN" optional="true"/>
        <attribute name="appId" type="String" mode="IN" optional="true"/>
        <attribute name="appVersion" type="String" mode="IN" optional="true"/>

        <attribute name="isActive" type="String" mode="IN" optional="false"/>
    </service>

    <service name="gnFindUserLoginDeviceUniqueIds" engine="java" auth="false"
             location="it.memelabs.gn.services.user.GnLoginServices" invoke="gnFindUserLoginDeviceUniqueIds"
             export="false">
        <description>gnFindUserLoginDeviceUniqueIds</description>
        <attribute name="userLoginId" type="String" mode="IN" optional="true"/>
        <attribute name="deviceId" type="String" mode="IN" optional="true"/>
        <attribute name="isActive" type="String" mode="IN" optional="true"/>
        <attribute name="list" type="java.util.List" mode="OUT" optional="false"/>
        <attribute name="listSize" type="java.lang.Integer" mode="OUT" optional="false"/>
    </service>


</services>