    <service name="gnCreateUpdatePartyNode" default-entity-name="GnPartyNode" engine="java"
             location="it.memelabs.gn.services.node.NodeServices" invoke="gnCreateUpdatePartyNode"
             auth="true" export="true">
        <description>Create or update an Abstract Node</description>
        <required-permissions join-type="OR">
            <permission-service service-name="gnAdminPermissionCheck" main-action="CREATE"/>
            <permission-service service-name="gnTreePermissionCheck" main-action="CREATE"/>
            <permission-service service-name="gnNodePermissionCheck" main-action="CREATE"/>

            <permission-service service-name="gnAdminPermissionCheck" main-action="UPDATE"/>
            <permission-service service-name="gnTreePermissionCheck" main-action="UPDATE"/>
            <permission-service service-name="gnNodePermissionCheck" main-action="UPDATE"/>

            <permission-service service-name="gnCompanyPermissionCheck" main-action="UPDATE"/>
            <permission-service service-name="gnCompanyTreePermissionCheck" main-action="UPDATE"/>

            <permission-service service-name="gnCompanyPermissionCheck" main-action="CREATE"/>
            <permission-service service-name="gnCompanyTreePermissionCheck" main-action="CREATE"/>
        </required-permissions>

        <implements service="gnAuditEventInterface"/>


        <!--
        lastModifiedDate			java.sql.Timestamp, TIMESTAMPTZ
        groupName					String, TEXT
        description					String, TEXT
        lastModifiedByUserLogin		String, TEXT
        partyId*					String, TEXT
        validator					String, CHAR(1)
        baseEndpointUrl				String, TEXT
        remote						String, CHAR(1)
        nodeType					String, TEXT
        code						String, TEXT
        taxIdentificationNumber		String, TEXT
        VATnumber					String, TEXT
        name						String, TEXT
        activityNumber				String, TEXT
        registeredOffice			String, CHAR(1)
        authorizationOwner			String, CHAR(1)
        reaNumber					String, TEXT
        -->
        <auto-attributes include="all" mode="IN" optional="true" entity-name="GnPartyGroupPartyNode">
            <exclude field-name="lastModifiedDate"/>
            <exclude field-name="lastModifiedByUserLogin"/>
        </auto-attributes>
        <attribute name="address" type="java.util.Map" mode="IN" optional="true"/>
        <attribute name="contact" type="java.util.Map" mode="IN" optional="true"/>
        <attribute name="partyRelationships" type="java.util.Collection" mode="IN" optional="true"/>

        <attribute name="partyId" type="String" mode="OUT" optional="false"/>
        <attribute name="nodeKey" type="String" mode="OUT" optional="false"/>
        <attribute name="companyBase" type="java.util.Map" mode="OUT" optional="true"/>
        <attribute name="partyRelationshipIds" type="java.util.Collection" mode="OUT" optional="true"/>
        <attribute name="reversePartyRelationshipIds" type="java.util.Collection" mode="OUT" optional="true"/>
    </service>

