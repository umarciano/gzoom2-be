<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Gn Services</description>
    <vendor></vendor>
    <version>1.0</version>


    <service name="gnTestFindServiceInvocationLog" default-entity-name="" engine="java"
             location="it.memelabs.gn.services.system.GnSystemServices" invoke="gnFindServiceInvocationLog"
             auth="true" export="true">
        <permission-service service-name="gnTestUserPermissionCheck"/>
        <implements service="gnFindServiceInvocationLog"/>
    </service>

    <service name="gnTestCreateServiceInvocationLog" default-entity-name="" engine="java"
             location="it.memelabs.gn.services.system.GnSystemServices" invoke="gnCreateServiceInvocationLog"
             auth="true" export="true">
        <permission-service service-name="gnTestUserPermissionCheck"/>
        <implements service="gnCreateServiceInvocationLog"/>
    </service>

    <service name="gnTestCreateJob" engine="java" auth="true"
             location="it.memelabs.gn.services.job.JobServices" invoke="gnCreateJob"
             export="true">
        <description>Create an activityJob</description>
        <permission-service service-name="gnTestUserPermissionCheck"/>
        <implements service="gnCreateJob"/>
    </service>

    <service name="gnTestConsumeJobs" engine="java" auth="true"
             location="it.memelabs.gn.services.job.JobServices" invoke="gnTestConsumeJobs"
             export="true">
        <description>Consume jobs</description>
        <attribute name="jobToConsume" type="java.lang.Long" mode="IN" optional="false"/>
        <attribute name="dateToConsume" type="java.sql.Timestamp" mode="IN" optional="false"/>
        <attribute name="consumedJobList" type="java.util.List" mode="OUT" optional="false"/>
        <attribute name="consumedJobListSize" type="java.lang.Integer" mode="OUT" optional="false"/>
    </service>

    <service name="gnTestFindJobs" engine="java" auth="true"
             location="it.memelabs.gn.services.job.JobServices" invoke="gnFindJobs"
             export="true">
        <description>gnFindJobs proxy for unit test</description>
        <implements service="gnFindJobs"/>
    </service>


</services>



