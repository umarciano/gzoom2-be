            <xml-property name="queryText"><![CDATA[SELECT 
	PP.PARENT_ROLE_CODE AS PARENT_ROLE_CODE, 
	P.PARTY_NAME AS PARTY_NAME, 
	P.PARTY_ID, 
	R.ROLE_TYPE_ID_TO,
	R.THRU_DATE,
	RT1.DESCRIPTION AS TIPO_RUOLO

FROM PARTY_RELATIONSHIP R
INNER JOIN PARTY P ON P.PARTY_ID = R.PARTY_ID_TO
INNER JOIN PARTY_PARENT_ROLE PP ON PP.PARTY_ID = P.PARTY_ID
INNER JOIN ROLE_TYPE RP ON RP.ROLE_TYPE_ID = PP.ROLE_TYPE_ID
INNER JOIN PARTY_ROLE PR ON PR.PARTY_ID = PP.PARTY_ID
	AND PR.PARENT_ROLE_TYPE_ID = PP.ROLE_TYPE_ID
INNER JOIN ROLE_TYPE RT1 ON RT1.ROLE_TYPE_ID = PR.ROLE_TYPE_ID

WHERE RP.ROLE_TYPE_ID = 'ORGANIZATION_UNIT' 	
	AND R.PARTY_RELATIONSHIP_TYPE_ID = 'GROUP_ROLLUP' 
	AND R.THRU_DATE IS NULL
	AND P.PARTY_ID <> 'Company'
	AND (? IS NULL OR ? = '' OR ? = P.PARTY_ID)
ORDER BY
	RT1.DESCRIPTION,
	P.PARTY_NAME	
	]]></xml-property>
