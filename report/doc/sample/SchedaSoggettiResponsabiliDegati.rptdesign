            <xml-property name="queryText"><![CDATA[SELECT 
	RT.DESCRIPTION AS RUOLO, 
	PP.PARENT_ROLE_CODE AS CODICE, 
	P.PARTY_NAME AS DESCRIZIONE, 
	PR.FROM_DATE, 
	PR.THRU_DATE
	
FROM PARTY_RELATIONSHIP PR  
INNER JOIN PARTY P ON P.PARTY_ID = PR.PARTY_ID_TO
INNER JOIN ROLE_TYPE RT ON RT.ROLE_TYPE_ID = PR.ROLE_TYPE_ID_TO
INNER JOIN PARTY_PARENT_ROLE PP ON  PP.PARTY_ID = P.PARTY_ID

		
WHERE 
	PR.PARTY_RELATIONSHIP_TYPE_ID = 'ORG_DELEGATE' 
	AND ? = PR.PARTY_ID_FROM 
	AND ? = PR.ROLE_TYPE_ID_FROM
	AND PR.THRU_DATE IS NULL   

ORDER BY 
	PP.PARENT_ROLE_CODE,
	P.PARTY_NAME]]></xml-property>
