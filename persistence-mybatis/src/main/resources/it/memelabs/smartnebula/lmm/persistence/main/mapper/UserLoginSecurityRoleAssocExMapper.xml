<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="it.memelabs.smartnebula.lmm.persistence.main.mapper.UserLoginSecurityRoleAssocExMapper">
    <select id="selectUserLoginId"
            resultMap="it.memelabs.smartnebula.lmm.persistence.main.mapper.UserLoginSecurityRoleAssocMapper.BaseResultMap"
            parameterType="long">
        select
        <include
                refid="it.memelabs.smartnebula.lmm.persistence.main.mapper.UserLoginSecurityRoleAssocMapper.Base_Column_List"/>
        from user_login_security_role_assoc
        WHERE user_login_id=#{userLoginId,jdbcType=BIGINT}
    </select>

    <insert id="bulkInsert">
        insert into user_login_security_role_assoc (user_login_id, node_id, security_role_id)
        values
        <foreach collection="items" item="model" separator=",">
            (#{model.userLoginId,jdbcType=BIGINT},
            #{model.nodeId,jdbcType=BIGINT},
            #{model.securityRoleId,jdbcType=BIGINT})
        </foreach>
    </insert>
</mapper>